Public Function Encrypt(ByVal Cadena As String) As String
    Dim X As Long, tLen As Integer, newString As String
    tLen = Len(Cadena)
    For X = 1 To tLen
        newString = newString & Chr$(Asc(mid(Cadena, X, 1)) + 47)        ' asci = 47
    Next X
    Encrypt = newString
End Function
Public Function Decrypt(ByVal Cadena As String) As String
    Dim X As Long, tLen As Integer, newString As String
    tLen = Len(Cadena)
    For X = 1 To tLen
        newString = newString & Chr$(Asc(mid(Cadena, X, 1)) - 47)        ' asci = 47
    Next X
    Decrypt = newString
End Function

'SuperMid(strBuffer, "[", "]", False)
Public Function SuperMid(ByVal strMain As String, str1 As String, str2 As String, Optional reverse As Boolean) As String

'DEVELOPER: Ryan Wells (wellsr.com)

    Dim i As Integer, j As Integer, Temp As Variant

    On Error GoTo ErrHandler:

    If reverse = True Then
        i = InStrRev(strMain, str1)
        j = InStrRev(strMain, str2)

        If Abs(j - i) < Len(str1) Then j = InStrRev(strMain, str2, i)
        If i = j Then    'try to search 2nd half of string for unique match
            j = InStrRev(strMain, str2, i - 1)

        End If

    Else
        i = InStr(1, strMain, str1)
        j = InStr(1, strMain, str2)

        If Abs(j - i) < Len(str1) Then j = InStr(i + Len(str1), strMain, str2)
        If i = j Then    'try to search 2nd half of string for unique match
            j = InStr(i + 1, strMain, str2)

        End If

    End If

    If i = 0 And j = 0 Then GoTo ErrHandler:
    If j = 0 Then j = Len(strMain) + Len(str2)    'just to make it arbitrarily large
    If i = 0 Then i = Len(strMain) + Len(str1)    'just to make it arbitrarily large
    If i > j And j <> 0 Then    'swap order
        Temp = j
        j = i
        i = Temp
        Temp = str2
        str2 = str1
        str1 = Temp

    End If

    i = i + Len(str1)
    SuperMid = mid(strMain, i, j - i)
    Exit Function
ErrHandler:
    SuperMid = "A"
End Function

Public Function BorrarMensaje(ByVal phrase As String)
    Dim startPos As Long
    Dim endPos As Long
    Dim searchPos As Long
    Dim findPos As Long

    Do While Not InStr(1, frmMain.RecTxt.Text, phrase, vbTextCompare) = 0
        frmMain.RecTxt.Locked = True
        startPos = 1
        endPos = Len(frmMain.RecTxt.Text)
        Do While startPos < endPos
            findPos = InStr(startPos, frmMain.RecTxt.Text, phrase, vbTextCompare)
            If findPos = 0 Then Exit Do
            startPos = findPos
            endPos = startPos + Len(phrase)
            frmMain.RecTxt.SelStart = startPos - 1
            frmMain.RecTxt.SelLength = Len(phrase)
            frmMain.RecTxt.SelText = ""
            startPos = startPos + Len(phrase)
        Loop
        frmMain.RecTxt.Locked = False
    Loop

End Function

Public Function ReemplazarMensaje(ByVal phrase As String, ByVal replacement As String)
    Dim startPos As Long
    Dim endPos As Long
    Dim searchPos As Long
    Dim findPos As Long

    frmMain.RecTxt.Locked = True

    startPos = 1
    endPos = Len(frmMain.RecTxt.Text)

    Do While startPos < endPos
        findPos = InStr(startPos, frmMain.RecTxt.Text, phrase, vbTextCompare)

        If findPos = 0 Then Exit Do

        startPos = findPos
        endPos = startPos + Len(phrase)

        frmMain.RecTxt.SelStart = startPos - 1
        frmMain.RecTxt.SelLength = Len(phrase)
        frmMain.RecTxt.SelText = replacement

        startPos = startPos + Len(replacement)
    Loop

    frmMain.RecTxt.Locked = False
End Function

