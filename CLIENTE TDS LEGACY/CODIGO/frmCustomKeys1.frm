Attribute VB_Name = "frmCambiarTeclas"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Private clsFormulario As clsFrmMovMan

Private Sub Form_Load()

    Call forms_load_pic(Me, "963.bmp", False)

    Dim I As Long

    ' Handles Form movement (drag and drop).
    Set clsFormulario = New clsFrmMovMan
    clsFormulario.Initialize Me

    For I = 1 To Teclas.Count
        If I <> 24 And I <> 19 Then
            Text1(I).Text = Teclas.ReadableName(Teclas.BindedKey(I))
        End If
    Next I

End Sub

Private Sub imgClose_Click()
    Unload Me
End Sub

Private Sub imgDefault_Click()
    Call Teclas.LoadDefaults
    Dim I As Long

    For I = 1 To Teclas.Count
        If I <> 24 And I <> 19 Then
            Text1(I).Text = Teclas.ReadableName(Teclas.BindedKey(I))
        End If
    Next I
End Sub

Private Sub imgSave_Click()
    Dim I As Long
    For I = 1 To Teclas.Count

        If LenB(Text1(I).Text) = 0 And I <> 24 And I <> 19 Then
            Call MsgBox("Hay una o más teclas no válidas, por favor verifique.", vbCritical Or vbOKOnly Or vbApplicationModal Or vbDefaultButton1, "TDS Legacy")
            Exit Sub
        End If
    Next I
    Call Teclas.SaveTeclas
    Unload Me
End Sub

Private Sub Text1_KeyDown(index As Integer, KeyCode As Integer, Shift As Integer)
    Dim I As Long

    If LenB(Teclas.ReadableName(KeyCode)) = 0 Then Exit Sub
    'If key is not valid, we exit

    Text1(index).Text = Teclas.ReadableName(KeyCode)
    Text1(index).SelStart = Len(Text1(index).Text)

    For I = 1 To Teclas.Count
        If I <> index And I <> 24 And I <> 19 Then
            If Teclas.BindedKey(I) = KeyCode Then
                Text1(index).Text = ""        'If the key is already assigned, simply reject it
                Call Beep        'Alert the user
                KeyCode = 0
                Exit Sub
            End If
        End If
    Next I

    Teclas.BindedKey(index) = KeyCode
End Sub

Private Sub Text1_KeyPress(index As Integer, KeyAscii As Integer)
    KeyAscii = 0
End Sub

Private Sub Text1_KeyUp(index As Integer, KeyCode As Integer, Shift As Integer)
    Call Text1_KeyDown(index, KeyCode, Shift)
End Sub

